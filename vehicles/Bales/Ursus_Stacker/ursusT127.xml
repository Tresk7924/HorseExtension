        <i3dMapping id="baleJointRow2" node="0>0|3|0|3|1" />
        <i3dMapping id="baleJointRow3" node="0>0|3|0|3|2" />
        <i3dMapping id="baleJointRow4" node="0>0|3|0|3|3" />
