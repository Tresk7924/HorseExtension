    <growth overwrite="true"> <!--Leave as false, if you are only appending new fruits. If set to true, you must make sure to define the growth for all fruits -->
        <defaultCrops>
            <defaultCrop name="ALFALFA"/>
            <defaultCrop name="BARLEY"/>
            <defaultCrop name="CANOLA"/>
            <defaultCrop name="CARROT"/>
            <defaultCrop name="CLOVER"/>
            <defaultCrop name="DRYGRASS" />
            <defaultCrop name="FIELD_GRASS"/>
            <defaultCrop name="GRASS"/>
            <defaultCrop name="HORSEGRASS"/>
            <defaultCrop name="MAIZE"/>
            <defaultCrop name="MISCANTHUS"/>
            <defaultCrop name="OAT"/>
            <defaultCrop name="OILSEEDRADISH" />
            <defaultCrop name="ONION"/>
            <defaultCrop name="PASTUREGRASS"/>
            <defaultCrop name="POPLAR"/>
            <defaultCrop name="POTATO"/>
            <defaultCrop name="RYE"/>
            <defaultCrop name="SOYBEAN"/>
            <defaultCrop name="SPELT"/>
            <defaultCrop name="SUGARBEET"/>
            <defaultCrop name="SUNFLOWER"/>
            <defaultCrop name="TRITICALE"/>
            <defaultCrop name="WEED" />
            <defaultCrop name="WHEAT"/>
            <defaultCrop name="COTTON"/>
            <defaultCrop name="SUGARCANE"/>
        </defaultCrops>

        <growthTransitions>
            <gt index="1"> <!--late winter to early spring-->
                <crop name="BARLEY"  incrementByOneRange="1-3"/>
                <crop name="SPELT" incrementByOneRange="1-2"/>
                <crop name="RYE"  incrementByOneRange="1-2"/>
                <crop name="TRITICALE"  incrementByOneRange="1-2"/>
                <crop name="OAT"  incrementByOneRange="1-2"/>
                <crop name="WHEAT"   incrementByOneRange="1-2"/>
                <crop name="CANOLA"    incrementByOneRange="1-2"/>
                <crop name="POPLAR" incrementByOneRange="1-MAX"/>
                <crop name="OILSEEDRADISH" incrementByOneRange="1-MAX"/>
				<crop name="FIELD_GRASS" incrementByOneRange="2-MAX"/>
                <crop name="MISCANTHUS" incrementByOneRange="2-MAX"/>
            </gt>
            <gt index="2"> <!--early spring to mid spring-->
                <crop name="BARLEY"  incrementByOneRange="1-4"/>
                <crop name="SPELT"  incrementByOneRange="1-3"/>
                <crop name="RYE"  incrementByOneRange="1-3"/>
                <crop name="TRITICALE"  incrementByOneRange="1-3"/>
                <crop name="OAT"  incrementByOneRange="1-3"/>
                <crop name="WHEAT"   incrementByOneRange="1-3"/>
                <crop name="CANOLA"    incrementByOneRange="1-3"/>
                <crop name="MAIZE" incrementByOneRange="1"/>
                <crop name="POTATO"    incrementByOneRange="1"/>
                <crop name="SUGARBEET" incrementByOneRange="1"/>
                <crop name="POPLAR" incrementByOneRange="1-MAX" setRange="CUT" setTo="2"/>
				<crop name="GRASS" incrementByOneRange="1-3" setRange="CUT"  setTo="2"/>
                <crop name="PASTUREGRASS" incrementByOneRange="1-3" setRange="CUT"  setTo="2"/>
				<crop name="FIELD_GRASS" incrementByOneRange="2-MAX" />
                <crop name="OILSEEDRADISH" incrementByOneRange="1-MAX"/>
				<crop name="ALFALFA" incrementByOneRange="1-3" setRange="CUT"  setTo="2"/>
				<crop name="CLOVER" incrementByOneRange="1-3" setRange="CUT"  setTo="2"/>
				<crop name="CARROT" incrementByOneRange="1"/>
                <crop name="ONION" incrementByOneRange="1"/>
                <crop name="MISCANTHUS" incrementByOneRange="2-MAX"/>
            </gt>
            <gt index="3"> <!--mid spring to late spring-->
                <crop name="BARLEY" incrementByOneRange="2-5"/>
                <crop name="SPELT"  incrementByOneRange="1-4"/>
                <crop name="RYE"  incrementByOneRange="1-4"/>
                <crop name="TRITICALE"  incrementByOneRange="1-4"/>
                <crop name="OAT"  incrementByOneRange="1-4"/>
                <crop name="WHEAT"   incrementByOneRange="1-4"/>
                <crop name="CANOLA"    incrementByOneRange="1-4"/>
                <crop name="MAIZE" incrementByOneRange="1-2"/>
				<crop name="GRASS" incrementByOneRange="1-MAX" setRange="CUT"  setTo="2"/>
				<crop name="PASTUREGRASS" incrementByOneRange="1-MAX" setRange="CUT"  setTo="2"/>
				<crop name="ALFALFA" incrementByOneRange="1-MAX" setRange="CUT"  setTo="2"/>
				<crop name="CLOVER" incrementByOneRange="1-MAX" setRange="CUT"  setTo="2"/>
				<crop name="FIELD_GRASS" incrementByOneRange="2-MAX" incrementByRange="4" incrementBy="2" setRange="CUT"  setTo="2"/>
				<crop name="HORSEGRASS" incrementByOneRange="1-MAX" setRange="CUT"  setTo="2"/>
                <crop name="SOYBEAN" incrementByOneRange="1"/>
                <crop name="SUNFLOWER" incrementByOneRange="1"/>
                <crop name="POTATO"    incrementByOneRange="1-2"/>
                <crop name="SUGARBEET" incrementByOneRange="1-2"/>
                <crop name="OILSEEDRADISH" incrementByOneRange="1-MAX"/>
				<crop name="ONION"    incrementByOneRange="1-2"/>
                <crop name="CARROT" incrementByOneRange="1-2"/>
                <crop name="MISCANTHUS"  incrementByOneRange="2-MAX"/>
            </gt>
            <gt index="4"> <!--late spring to early summer-->
                <crop name="BARLEY" incrementByOneRange="6" incrementByRange="3-5" incrementBy="2"/>
                <crop name="SPELT" incrementByOneRange="2-5"/>
                <crop name="RYE" incrementByOneRange="2-5"/>
                <crop name="TRITICALE" incrementByOneRange="2-5"/>
                <crop name="OAT" incrementByOneRange="2-5"/>
                <crop name="WHEAT" incrementByOneRange="2-5"/>
                <crop name="CANOLA" incrementByOneRange="2-5"/>
                <crop name="MAIZE" incrementByOneRange="1-3"/>
                <crop name="SOYBEAN" incrementByOneRange="1-2"/>
                <crop name="SUNFLOWER" incrementByOneRange="1-2"/>
                <crop name="POTATO"    incrementByOneRange="1-3"/>
                <crop name="SUGARBEET" incrementByOneRange="1-3"/>
                <crop name="POPLAR" incrementByOneRange="1-MAX" setRange="CUT" setTo="2" />
                <crop name="GRASS" incrementByOneRange="1-MAX" setRange="CUT"  setTo="2"/>
                <crop name="OILSEEDRADISH" incrementByOneRange="1-MAX"/>
				<crop name="ALFALFA" incrementByOneRange="1-MAX" setRange="CUT"  setTo="2"/>
				<crop name="CLOVER" incrementByOneRange="1-MAX" setRange="CUT"  setTo="2"/>
				<crop name="ONION"    incrementByOneRange="1-3"/>
                <crop name="CARROT" incrementByOneRange="1-3"/>
				<crop name="FIELD_GRASS" incrementByOneRange="2-MAX"/>
                <crop name="MISCANTHUS"  setRange="5-MAX" setTo="4"/>
            </gt>
            <gt index="5"> <!--early summer to mid summer-->
                <crop name="BARLEY" incrementByOneRange="6" incrementByRange="4-5" incrementBy="2"/>
                <crop name="SPELT" incrementByOneRange="6" incrementByRange="3-5" incrementBy="2"/>
                <crop name="RYE" incrementByOneRange="6" incrementByRange="3-5" incrementBy="2"/>
                <crop name="TRITICALE" incrementByOneRange="6" incrementByRange="3-5" incrementBy="2"/>
                <crop name="OAT" incrementByOneRange="6" incrementByRange="3-5" incrementBy="2"/>
                <crop name="WHEAT" incrementByOneRange="6" incrementByRange="3-5" incrementBy="2"/>
                <crop name="CANOLA" incrementByOneRange="6" incrementByRange="3-5" incrementBy="2"/>
                <crop name="MAIZE" incrementByOneRange="1-4" />
                <crop name="SOYBEAN" incrementByOneRange="1-3"/>
                <crop name="SUNFLOWER" incrementByOneRange="1-3" />
                <crop name="POTATO"    incrementByOneRange="1-4" />
                <crop name="SUGARBEET" incrementByOneRange="1-4" />
				<crop name="GRASS" incrementByOneRange="1-MAX" setRange="CUT"  setTo="2"/>
                <crop name="PASTUREGRASS" incrementByOneRange="1-MAX" setRange="CUT"  setTo="2"/>
				<crop name="ALFALFA" incrementByOneRange="1-MAX" setRange="CUT"  setTo="2"/>
				<crop name="CLOVER" incrementByOneRange="1-MAX" setRange="CUT"  setTo="2"/>
				<crop name="ONION"    incrementByOneRange="1-4" />
                <crop name="CARROT" incrementByOneRange="1-4" />
				<crop name="FIELD_GRASS" incrementByOneRange="2-4" setRange="CUT"  setTo="2"/>
				<crop name="HORSEGRASS" incrementByOneRange="1-MAX" setRange="CUT"  setTo="2"/>
				<crop name="MISCANTHUS" setRange="CUT" setTo="2"/>
                <!-- <crop name="WEED" /> -->
            </gt>
            <gt index="6"> <!--mid summer to late summer-->
                <crop name="BARLEY" incrementByOneRange="6" setRange="1" setTo="CUT"/>
                <crop name="SPELT" incrementByOneRange="6" incrementByRange="4-5" incrementBy="2"/>
                <crop name="RYE" incrementByOneRange="6" incrementByRange="4-5" incrementBy="2"/>
                <crop name="TRITICALE" incrementByOneRange="6" incrementByRange="4-5" incrementBy="2"/>
                <crop name="OAT" incrementByOneRange="6" incrementByRange="4-5" incrementBy="2"/>
                <crop name="WHEAT"  incrementByOneRange="6" incrementByRange="4-5" incrementBy="2"/>
                <crop name="CANOLA" incrementByOneRange="6" incrementByRange="4-5" incrementBy="2"/>
                <crop name="MAIZE" incrementByRange="2-5" incrementBy="2" incrementByOneRange="1"/>
                <crop name="SOYBEAN" incrementByRange="2-4" incrementBy="2" incrementByOneRange="1"/>
                <crop name="SUNFLOWER" incrementByRange="3-4" incrementBy="2" setRange="1"  setTo="CUT"/>
                <crop name="POTATO"    incrementByOneRange="3-5" />
                <crop name="SUGARBEET" incrementByOneRange="3-5" />
				<crop name="GRASS" incrementByOneRange="1-MAX" setRange="CUT"  setTo="2"/>
                <crop name="PASTUREGRASS" incrementByOneRange="1-MAX" setRange="CUT"  setTo="2"/>
				<crop name="ALFALFA" incrementByOneRange="1-MAX" setRange="CUT"  setTo="2"/>
				<crop name="CLOVER" incrementByOneRange="1-MAX" setRange="CUT"  setTo="2"/>
				<crop name="ONION"    incrementByOneRange="3-5" />
                <crop name="CARROT" incrementByOneRange="3-5" />
				<crop name="FIELD_GRASS" incrementByOneRange="2-4" setRange="6"  setTo="5" />
				<crop name="HORSEGRASS" incrementByOneRange="1-MAX" setRange="CUT"  setTo="2"/>
                <!-- <crop name="WEED" /> -->
            </gt>
            <gt index="7"> <!--late summer to early autumn-->
                <crop name="BARLEY" incrementByOneRange="1" setRange="2-MAX" setTo="WITHERED"/>
                <crop name="SPELT" incrementByOneRange="6" setRange="1" setTo="CUT"/>
                <crop name="RYE" incrementByOneRange="6" setRange="1" setTo="CUT"/>
                <crop name="TRITICALE" incrementByOneRange="6" setRange="1" setTo="CUT"/>
                <crop name="OAT" incrementByOneRange="6" setRange="1" setTo="CUT"/>
                <crop name="WHEAT" incrementByOneRange="6" setRange="1" setTo="CUT"/>
                <crop name="CANOLA" incrementByOneRange="6" setRange="1" setTo="CUT"/>
                <crop name="MAIZE" incrementByOneRange="1-6"/>
                <crop name="SOYBEAN" incrementByOneRange="1-6"/>
                <crop name="SUNFLOWER" incrementByOneRange="5-6"/>
                <crop name="POTATO"    incrementByOneRange="4-6"/>
                <crop name="SUGARBEET" incrementByOneRange="4-6"/>
                <crop name="POPLAR" setRange="CUT" setTo="2" incrementByOneRange="1-MAX"/>
				<crop name="GRASS" incrementByOneRange="2-MAX" setRange="CUT"  setTo="2"/>
                <crop name="PASTUREGRASS" incrementByOneRange="2-MAX" setRange="CUT"  setTo="2"/>
                <crop name="OILSEEDRADISH" incrementByOneRange="2-MAX"/>
				<crop name="ALFALFA" incrementByOneRange="2-MAX" setRange="CUT"  setTo="2"/>
				<crop name="CLOVER" incrementByOneRange="2-MAX" setRange="CUT"  setTo="2"/>
				<crop name="ONION"    incrementByOneRange="4-6"/>
                <crop name="CARROT" incrementByOneRange="4-6"/>
				<crop name="FIELD_GRASS" incrementByOneRange="1-4" setRange="CUT"  setTo="2"/>
				<crop name="HORSEGRASS" incrementByOneRange="2-MAX" setRange="CUT"  setTo="2"/>
				<crop name="MISCANTHUS"  incrementByOneRange="1-2"/>
                <!-- <crop name="WEED" /> -->
            </gt>
            <gt index="8"> <!--early autumn to mid autumn-->
                <crop name="BARLEY" incrementByOneRange="1" setRange="2-MAX" setTo="WITHERED"/>
                <crop name="SPELT" incrementByOneRange="1" setRange="2-MAX" setTo="WITHERED"/>
                <crop name="RYE" incrementByOneRange="1" setRange="2-MAX" setTo="WITHERED"/>
                <crop name="TRITICALE" incrementByOneRange="1" setRange="2-MAX" setTo="WITHERED"/>
                <crop name="OAT" incrementByOneRange="1" setRange="2-MAX" setTo="WITHERED"/>
                <crop name="WHEAT" incrementByOneRange="1" setRange="2-MAX" setTo="WITHERED"/>
                <crop name="CANOLA" incrementByOneRange="1" setRange="2-MAX" setTo="WITHERED"/>
                <crop name="MAIZE" incrementByOneRange="1-6" />
                <crop name="SOYBEAN" incrementByOneRange="5-6" />
                <crop name="SUNFLOWER" incrementByOneRange="6" />
                <crop name="POTATO"    incrementByOneRange="5-6" />
                <crop name="SUGARBEET" incrementByOneRange="5-6" />
                <crop name="OILSEEDRADISH" incrementByOneRange="1-MAX"/>
				<crop name="ONION"    incrementByOneRange="5-6" />
                <crop name="CARROT" incrementByOneRange="5-6" />
				<crop name="FIELD_GRASS" incrementByOneRange="1-3" />
				<crop name="MISCANTHUS"  incrementByOneRange="1-2"/>
            </gt>
            <gt index="9"> <!--mid autumn to late autumn-->
                <crop name="BARLEY"  incrementByOneRange="1" />
                <crop name="SPELT"  incrementByOneRange="1" />
                <crop name="RYE"  incrementByOneRange="1" />
                <crop name="TRITICALE"  incrementByOneRange="1" />
                <crop name="OAT"  incrementByOneRange="1" />
                <crop name="WHEAT"   incrementByOneRange="1" />
                <crop name="CANOLA"    incrementByOneRange="1" />
                <crop name="MAIZE" incrementByOneRange="6" />
                <crop name="SOYBEAN" incrementByOneRange="6"/>
                <!-- <crop name="SUNFLOWER" setRange="1"  setTo="CUT"/> -->
                <crop name="POTATO"     incrementByOneRange="6" />
                <crop name="SUGARBEET" incrementByOneRange="6" />
                <crop name="OILSEEDRADISH" incrementByOneRange="1-MAX"/>
				<crop name="ONION"     incrementByOneRange="6" />
                <crop name="CARROT" incrementByOneRange="6" />
				<crop name="FIELD_GRASS" incrementByOneRange="1-3" setRange="CUT"  setTo="2"/>
				<crop name="MISCANTHUS"  incrementByOneRange="1-2"/>
            </gt>
            <gt index="10"> <!--late autumn to early winter-->
                <crop name="BARLEY"  incrementByOneRange="1" />
                <crop name="RYE" incrementByOneRange="1"/>
                <crop name="SPELT" incrementByOneRange="1"/>
                <crop name="OAT" incrementByOneRange="1"/>
                <crop name="WHEAT" incrementByOneRange="1"/>
                <crop name="CANOLA" incrementByOneRange="1"/>
                <crop name="MAIZE" setRange="1-5"  setTo="CUT" incrementByOneRange="7"/>
                <crop name="SOYBEAN" setRange="1-5" setTo="CUT" incrementByOneRange="7"/>
                <crop name="SUNFLOWER" setRange="1-5" setTo="CUT" incrementByOneRange="7"/>
                <crop name="POTATO" setRange="1-5" setTo="CUT" incrementByOneRange="7"/>
                <crop name="PASTUREGRASS" setRange="1-CUT" setTo="2"/>
				<crop name="ALFALFA" setRange="1-CUT" setTo="2"/>
				<crop name="CLOVER" setRange="1-CUT" setTo="2"/>
				<crop name="ONION" setRange="1-4" setTo="CUT" incrementByOneRange="7"/>
                <crop name="CARROT" setRange="1-4" setTo="CUT" incrementByOneRange="7"/>
				<crop name="FIELD_GRASS" setRange="1-CUT" setTo="2"/>
				<crop name="HORSEGRASS" setRange="1-CUT" setTo="2"/>
				<crop name="GRASS" setRange="1-CUT" setTo="2"/>
				<crop name="MISCANTHUS"  incrementByOneRange="2-3"/>
            </gt>
            <gt index="11"> <!--early winter to mid winter-->
                <crop name="BARLEY" setRange="1" setTo="CUT"/>
				<crop name="MISCANTHUS"  incrementByOneRange="2-3"/>
                <crop name="SUGARBEET" setRange="1-5" setTo="CUT" incrementByOneRange="7"/>
                <crop name="SUNFLOWER" setRange="1-MAX" setTo="CUT"/>
                <crop name="POTATO" setRange="1-MAX" setTo="CUT"/>
				<crop name="ONION" setRange="1-MAX" setTo="CUT"/>
                <crop name="CARROT" setRange="1-MAX" setTo="CUT"/>
				<crop name="MISCANTHUS"  incrementByOneRange="3"/>
            </gt>
            <gt index="12"> <!--mid winter to late winter-->
                <crop name="SUGARBEET" setRange="1-MAX" setTo="CUT"/>
				<crop name="MISCANTHUS" setRange="4" setTo="6"/>
            </gt>
            <gt index="FIRST_LOAD_TRANSITION">
                <crop name="BARLEY" setRange="1-MAX" setTo="CUT"/>
                <crop name="SPELT" setRange="1-MAX" setTo="CUT"/>
                <crop name="RYE" setRange="1-MAX" setTo="CUT"/>
                <crop name="TRITICALE" setRange="1-MAX" setTo="CUT"/>
                <crop name="OAT" setRange="1-MAX" setTo="CUT"/>
                <crop name="WHEAT" setRange="1-MAX" setTo="CUT"/>
                <crop name="CANOLA" setRange="1-MAX" setTo="CUT"/>
                <crop name="MAIZE" setRange="1-MAX" setTo="CUT"/>
                <crop name="SOYBEAN" setRange="1-MAX" setTo="CUT"/>
                <crop name="SUNFLOWER" setRange="1-MAX" setTo="CUT"/>
                <crop name="POTATO" setRange="1-MAX" setTo="WITHERED"/>
                <crop name="SUGARBEET" setRange="1-MAX" setTo="WITHERED"/>
                <crop name="POPLAR" setRange="1-MAX" setTo="CUT"/>
                <crop name="PASTUREGRASS" setRange="1-MAX" setTo="2"/>
                <crop name="OILSEEDRADISH" setRange="1-MAX" setTo="CUT"/>
				<crop name="ALFALFA" setRange="1-MAX" setTo="2"/>
				<crop name="CLOVER" setRange="1-MAX" setTo="2"/>
				<crop name="ONION" setRange="1-MAX" setTo="WITHERED"/>
                <crop name="CARROT" setRange="1-MAX" setTo="WITHERED"/>
				<crop name="FIELD_GRASS" setRange="1-MAX" setTo="2"/>
				<crop name="HORSEGRASS" setRange="1-MAX" setTo="2"/>
                <crop name="GRASS" setRange="1-MAX" setTo="2"/>
				<crop name="MISCANTHUS" setRange="1-MAX" setTo="2"/>
            </gt>
        </growthTransitions>
    </growth>

    <fruitTypes>
        <!-- when adding growth for new crops the following at a minimum need to be defined in this section.

        <fruitType name="NEW_CROP">
            <growth germinateTemp="5"
                    germinatesoilMoisture="5"
                    youngPlantMaxState="4"
                    maxFertiliseState="4"/>
            <options    seedDroughtResistanceFactor="1"
                        youngPlantDroughtResistanceFactor="2"
                        maturePlantDroughtResistanceFactor="3"
                        seedFrostResistanceFactor="3"
                        youngPlantFrostResistanceFactor="4"
                        maturePlantFrostResistanceFactor="1"/>
            <rotation category="CEREAL"
                      returnPeriod="2" />
        </fruitType> -->

        <!--resistance ratings: (float so 1.2 is possible). DO NOT SET ABOVE 4
        4 = resistant (no damage)
        3 = min damage
        2 = medium damage
        1 = max damage -->

        <!-- There is no need to include the below in your geo as it will be loaded by Seasons.
             if you want to change a value for a crop all you need to include is the value that you are changing.
             E.g. changing the germinaition temperature of Barley
            <fruitType name="BARLEY">
                <growth germinateTemp="3" />
            </fruitType> -->

        <fruitType name="BARLEY">
            <cultivation plantsWeed="true"/>
            <growth germinateTemp="5"
                    germinatesoilMoisture="5"
                    youngPlantMaxState="4"
                    maxFertiliseState="4"/>
            <options    seedDroughtResistanceFactor="1"
                        youngPlantDroughtResistanceFactor="2"
                        maturePlantDroughtResistanceFactor="3"
                        seedFrostResistanceFactor="3"
                        youngPlantFrostResistanceFactor="4"
                        maturePlantFrostResistanceFactor="1"/>
            <rotation category="CEREAL"
                      returnPeriod="2" />
        </fruitType>
        <fruitType name="SPELT">
            <cultivation plantsWeed="true"/>
            <growth germinateTemp="5"
                    germinatesoilMoisture="5"
                    youngPlantMaxState="4"
                    maxFertiliseState="4"/>
            <options    seedDroughtResistanceFactor="1"
                        youngPlantDroughtResistanceFactor="2"
                        maturePlantDroughtResistanceFactor="3"
                        seedFrostResistanceFactor="3"
                        youngPlantFrostResistanceFactor="4"
                        maturePlantFrostResistanceFactor="1"/>
            <rotation category="CEREAL"
                      returnPeriod="2" />
        </fruitType>
        <fruitType name="RYE">
            <cultivation plantsWeed="true"/>
            <growth germinateTemp="5"
                    germinatesoilMoisture="5"
                    youngPlantMaxState="4"
                    maxFertiliseState="4"/>
            <options    seedDroughtResistanceFactor="1"
                        youngPlantDroughtResistanceFactor="2"
                        maturePlantDroughtResistanceFactor="3"
                        seedFrostResistanceFactor="3"
                        youngPlantFrostResistanceFactor="4"
                        maturePlantFrostResistanceFactor="1"/>
            <rotation category="CEREAL"
                      returnPeriod="2" />
        </fruitType>
        <fruitType name="TRITICALE">
            <cultivation plantsWeed="true"/>
            <growth germinateTemp="5"
                    germinatesoilMoisture="5"
                    youngPlantMaxState="4"
                    maxFertiliseState="4"/>
            <options    seedDroughtResistanceFactor="1"
                        youngPlantDroughtResistanceFactor="2"
                        maturePlantDroughtResistanceFactor="3"
                        seedFrostResistanceFactor="3"
                        youngPlantFrostResistanceFactor="4"
                        maturePlantFrostResistanceFactor="1"/>
            <rotation category="CEREAL"
                      returnPeriod="2" />
        </fruitType>
        <fruitType name="OAT">
            <cultivation plantsWeed="true"/>
            <growth germinateTemp="6"
                    germinatesoilMoisture="5"
                    youngPlantMaxState="4"
                    maxFertiliseState="4"/>
            <options    seedDroughtResistanceFactor="1"
                        youngPlantDroughtResistanceFactor="2"
                        maturePlantDroughtResistanceFactor="3"
                        seedFrostResistanceFactor="3"
                        youngPlantFrostResistanceFactor="4"
                        maturePlantFrostResistanceFactor="1"/>
            <rotation category="CEREAL"
                      returnPeriod="2" />
        </fruitType>
        <fruitType name="WHEAT">
            <cultivation plantsWeed="true"/>
            <growth germinateTemp="5"
                    germinatesoilMoisture="5"
                    youngPlantMaxState="4"
                    maxFertiliseState="4"/>
            <options    seedDroughtResistanceFactor="1"
                        youngPlantDroughtResistanceFactor="2"
                        maturePlantDroughtResistanceFactor="3"
                        seedFrostResistanceFactor="3"
                        youngPlantFrostResistanceFactor="4"
